# AI Vision Configuration

# Google Cloud Vision API - АКТИВЕН ✅
# Путь к JSON файлу с credentials
GOOGLE_APPLICATION_CREDENTIALS=/workspaces/my_agent/gdd-suite-8ae26f5f3853.json
GOOGLE_CLOUD_PROJECT=gdd-suite

# Phi Vision 3.5 - АКТИВЕН ✅ 
# Локальная обработка через Hugging Face Transformers
PHI_VISION_ENABLED=true

# Hugging Face Token (опционально, для приватных моделей)
# Получите на: https://huggingface.co/settings/tokens
HUGGINGFACE_API_TOKEN=your_huggingface_token_here

# Claude Vision - ОТКЛЮЧЕН (нет API ключа)
ANTHROPIC_API_KEY=disabled

# OpenAI Vision - ОТКЛЮЧЕН (опционально)
OPENAI_API_KEY=disabled

# Настройки модели
PHI_MODEL_NAME=microsoft/Phi-3.5-vision-instruct
CLAUDE_MODEL_NAME=claude-3-5-sonnet-20241022
MAX_TOKENS=1000
TEMPERATURE=0.7
BATCH_SIZE=5

# Настройки приложения
DEBUG=True
LOG_LEVEL=INFO
DEVICE=auto
